<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <properties resource="generatorConfig.properties" />
    <classPathEntry  location="${mysql.driver.path}"/>
    <context id="testTables"  targetRuntime="MyBatis3">

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="${mysql.jdbc.connectionURL}" userId="${mysql.jdbc.username}" password="${mysql.jdbc.password}">
            <!-- <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/db_hl" userId="root" password=""> -->
        </jdbcConnection>
        <javaTypeResolver>
            <!--<property name="forceBigDecimals" value="false"/>-->
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.xk.product.dao.model" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="mappers" targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.xk.product.dao.mapper" targetProject="./src/main/java">

        </javaClientGenerator>

        <table tableName="trans_log" domainObjectName="TransLog">
            <!--enableCountByExample="false"
                           enableUpdateByExample="false"
                           enableDeleteByExample="false"
                           enableSelectByExample="false"
                           selectByExampleQueryId="false"-->
        </table>

    </context>
</generatorConfiguration>